// csignal standard header
#ifndef _CSIGNAL_
#define _CSIGNAL_

#pragma noarguments_in_registers(push)

#include <signal.h>
#include <yvals.h>
 #if _HAS_NAMESPACE
_STD_BEGIN
using ::sig_atomic_t; using ::raise; using ::signal;
_STD_END
 #else
 #endif /* _HAS_NAMESPACE */

#pragma arguments_in_registers(pop)

#endif /* _CSIGNAL_ */

