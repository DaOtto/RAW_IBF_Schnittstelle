// ctime standard header
#ifndef _CTIME_
#define _CTIME_

#pragma noarguments_in_registers(push)

#include <time.h>
#include <yvals.h>
 #if _HAS_NAMESPACE
_STD_BEGIN
using ::size_t; using ::clock_t; using ::time_t;
using ::tm; using ::time; using ::asctime;
using ::clock; using ::ctime; using ::difftime;
using ::gmtime; using ::localtime; using ::mktime;
using ::strftime;
_STD_END
 #else
 #endif /* _HAS_NAMESPACE */

#pragma arguments_in_registers(pop)

#endif /* _CTIME_ */

